{
  "problemMatcher": [
    {
      "owner": "erlc-warning-matcher",
      "pattern": [
        {
          "regexp": "^(\\S+):(\\d+):(\\d+): ([wW]arning): (.+)$$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "erlc-error-matcher",
      "pattern": [
        {
          "regexp": "^(\\S+):(\\d+):(\\d+): (.+)$$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "ct-problem-matcher",
      "pattern": [
        {
          "regexp": "^(\\S+) failed on line (\\d+)$$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": "^Reason: (.+)$$",
          "message": 1
        }
      ]
    },
    {
      "owner": "eunit-problem-matcher",
      "pattern": [
        {
          "regexp": "^\\S+: .+...\\*failed\\*$$"
        },
        {
          "regexp": "^in function .+ \\((.+), line (\\d+)\\)$$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": "^in call from .+$$"
        },
        {
          "regexp": "^\\*\\*error:(.+)$$",
          "message": 1
        }
      ]
    }
  ]
}
